
<html>
    <head>
        <title>Merchant Check Out Page</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    </head>
    <body>
        <center><h1>Please do not refresh this page...</h1></center>
        <form method="post" action="https://securegw.paytm.in/theia/processTransaction?ORDER_ID=<%= params['ORDER_ID'] %>" name="f1">
            <table border="1">
                <tbody>
                    <input type="hidden" name="MID" value="<%= params['MID'] %>">
                    <input type="hidden" name="WEBSITE" value="<%= params['WEBSITE'] %>">
                    <input type="hidden" name="CHANNEL_ID" value="<%= params['CHANNEL_ID'] %>">
                    <input type="hidden" name="INDUSTRY_TYPE_ID" value="<%= params['INDUSTRY_TYPE_ID'] %>">
                    <input type="hidden" name="ORDER_ID" value="<%= params['ORDER_ID'] %>">
                    <input type="hidden" name="CUST_ID" value="<%= params['CUST_ID'] %>">
                    <input type="hidden" name="TXN_AMOUNT" value="<%= params['TXN_AMOUNT'] %>">
                    <input type="hidden" name="CALLBACK_URL" value="<%= params['CALLBACK_URL'] %>">
                    <input type="hidden" name="CHECKSUMHASH" value="<%= checksum %>">
                </tbody>
            </table>
        <script type="text/javascript">
            document.f1.submit();
        </script>
        </form>
    </body>
</html>

